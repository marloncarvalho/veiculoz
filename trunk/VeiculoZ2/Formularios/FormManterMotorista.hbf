Private Sub Initialize()
	Me.RegrasNegocio = RNFactory.RNMotorista()
	Me.SelectorEmissao.Date = Now()
	Me.SelectorVcto.Date = Now()
End Sub

Private Sub clsFormManter_Preencher(ByRef Repassador As clsRepassador)
	Dim Motorista As clsMotorista
	Motorista = clsMotorista(Me.RegrasNegocio.ObterUltimo())
	If Motorista Is Nothing Then
		Motorista = New clsMotorista
	End If
	Motorista.Nome = Me.FieldNome.Text
	Motorista.Habilitacao = Me.FieldHabilitacao.Text
	Motorista.Categoria = Me.FieldCategoria.Text
	Motorista.Vencimento = Me.SelectorVcto.Date
	Motorista.Emissao = Me.SelectorEmissao.Date
	Repassador.Objeto = Motorista
End Sub

Private Sub clsFormManter_Validar(ByRef Objeto As clsObjetoGenerico, ByRef MsgErro As String)
	Dim Motorista As clsMotorista
	Set Motorista = clsMotorista(Objeto)
	If Motorista.Nome = "" Then
		MsgErro = "Informe o Campo Nome."
		Exit Sub
	End If
End Sub

Private Sub clsFormManter_PreencherCampos(ByRef Objeto As clsObjetoGenerico)
	Dim Motorista As New clsMotorista
	Set Motorista = clsMotorista(Objeto)
	Me.FieldNome.Text = Motorista.Nome
	Me.FieldHabilitacao.Text = Motorista.Habilitacao
	Me.FieldCategoria.Text = Motorista.Categoria
	Me.SelectorEmissao.Date = Motorista.Emissao
	Me.SelectorVcto.Date = Motorista.Vencimento
End Sub

Private Sub btnSalvar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Salvar()
End Sub

Private Sub btnVoltar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormInicial
	Form.Show(hbFormModal+hbFormGoto)
End Sub

Private Sub clsFormManter_LimparCampos()
	Me.FieldNome.Text = ""
	Me.FieldHabilitacao.Text = ""
	Me.FieldCategoria.Text = ""
	Me.SelectorEmissao.Date = Now()
	Me.SelectorVcto.Date = Now()
End Sub

Private Sub btnLembrete_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormManterLembrete
	Form.SelectorData.Date = Me.SelectorVcto.Date
	Form.FieldDescricao.Text = "Renovar Habilitação"
	Form.Show(hbFormPopup)
End Sub]]></Source>
