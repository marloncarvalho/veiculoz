Private Sub Initialize()
	Me.RegrasNegocio = RNFactory.RNVeiculos()
End Sub

Private Sub clsFormManter_Preencher(ByRef Repassador As clsRepassador)
	Dim Veiculo As New clsVeiculo
	Veiculo.Ano = Me.fieldAno.Text
	Veiculo.Chassi = Me.fieldChassi.Text
	Veiculo.Cor = Me.fieldCor.Text
	Veiculo.Marca = Me.fieldMarca.Text
	Veiculo.Modelo = Me.fieldModelo.Text
	Veiculo.Placa = Me.fieldPlaca.Text
	Veiculo.Renavam = Me.fieldRenavam.Text
	Veiculo.Id = Me.Identificador
	Set Repassador.Objeto = Veiculo
End Sub

Private Sub clsFormManter_Validar(ByRef Objeto As clsObjetoGenerico, ByRef MsgErro As String)
	Dim Veiculo As clsVeiculo
	Set Veiculo = clsVeiculo(Objeto)
	If Veiculo.Marca = "" Then
		MsgErro = "Informe a Marca do Ve√≠culo."
		Exit Sub
	End If
	If Veiculo.Modelo = "" Then
		MsgErro = "Informe o Modelo do Ve√≠culo."
		Exit Sub
	End If
End Sub

Private Sub clsFormManter_PreencherCampos(ByRef Objeto As clsObjetoGenerico)
	Dim Veiculo As clsVeiculo
	Set Veiculo = clsVeiculo(Objeto)
	Me.fieldAno.Text = Veiculo.Ano
	Me.fieldChassi.Text = Veiculo.Chassi
	Me.fieldCor.Text = Veiculo.Cor
	Me.fieldMarca.Text = Veiculo.Marca
	Me.fieldModelo.Text = Veiculo.Modelo
	Me.fieldPlaca.Text = Veiculo.Placa
	Me.fieldRenavam.Text = Veiculo.Renavam
	Me.Identificador = Veiculo.Id
End Sub

Private Sub btnSalvar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Salvar()
End Sub

Private Sub btnExcluir_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Excluir()
End Sub

Private Sub btnNovo_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.LimparCampos()
End Sub

Private Sub btnVoltar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Voltar()
End Sub

Private Sub clsFormManter_LimparCampos()
	Me.fieldAno.Text = ""
	Me.fieldChassi.Text = ""
	Me.fieldCor.Text = ""
	Me.fieldMarca.Text = ""
	Me.fieldModelo.Text = ""
	Me.fieldPlaca.Text = ""
	Me.fieldRenavam.Text = ""
	Me.Identificador = 0
	Me.Estado = 1
End Sub

Private Sub clsFormManter_TelaListagem(ByRef Tela As clsRepassador)
	Set Tela.Objeto = New FormListarVeiculos
End Sub

Private Sub btnSeguro_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormListarSeguros
	Form.Show(hbFormGoto+hbFormModal)
End Sub]]></Source>
