Private Sub Initialize()
	Set Me.RegrasNegocio = RNFactory.RNDespesas()
End Sub

Private Sub clsFormManter_Inicializar()
	ObterPostos(Me.PopupLocal)
	ObterTiposDespesas(Me.PopupTipo)
	ObterVeiculos(Me.PopupVeiculos)
	If Me.PopupVeiculos.ListCount > 0 Then
		Me.PopupVeiculos.ListIndex = 0
	End If
	Me.SelectorData.Date = Now()
	Me.SelectorHora.Time = DatePart("ss",Now())
End Sub

Private Sub clsFormManter_Preencher(ByRef Repassador As clsRepassador)
	Dim Despesa As New clsDespesa
	Dim Tipo As New clsTipoDespesa
	Dim Local As New clsEmpresa
	Dim Veiculo As New clsVeiculo
	Despesa.Id = Me.Identificador
	Despesa.Data = Me.SelectorData.Date
	Despesa.Hora = Me.SelectorData.Time
	Despesa.Descricao = Me.FieldDescricao.Text
	If Me.FieldProximaDespesa.Text <> "" Then
		Despesa.ProximaDespesa = CDbl(Me.FieldProximaDespesa.Text)
	End If
	If Me.FieldKM.Text <> "" Then
		Despesa.KM = Me.FieldKM.Text
	Else
		Despesa.KM = 0
	End If
	If Me.FieldValor.Text <> "" Then
		Despesa.Valor = CDbl(Me.FieldValor.Text)
	Else
		Despesa.Valor = 0
	End If
	If Me.PopupVeiculos.ListIndex > -1 Then
		Veiculo.Id = Me.PopupVeiculos.ItemData(Me.PopupVeiculos.ListIndex)
	End If
	If Me.PopupTipo.ListIndex > -1 Then
		Tipo.Id = Me.PopupTipo.ItemData(Me.PopupTipo.ListIndex)
	End If
	If Me.PopupLocal.ListIndex > -1 Then
		Local.Id = Me.PopupLocal.ItemData(Me.PopupLocal.ListIndex)
	End If
	Despesa.Veiculo = Veiculo
	Despesa.Tipo = Tipo
	Despesa.Local = Local
	Despesa.Observacoes = Me.FieldObservacoes.Text
	Set Repassador.Objeto = Despesa
End Sub

Private Sub clsFormManter_Validar(ByRef Objeto As clsObjetoGenerico, ByRef MsgErro As String)
	Dim Despesa As clsDespesa
	Set Despesa = clsDespesa(Objeto)
	If Despesa.Veiculo.Id <= 0 Then
		MsgErro = "Informe o Veículo."
		Exit Sub
	End If
	If Despesa.Local.Id <= 0 Then
		MsgErro = "Informe o Local."
		Exit Sub
	End If
	If Despesa.Tipo.Id <= 0 Then
		MsgErro = "Informe o Tipo de Despesa."
		Exit Sub
	End If
	If Despesa.Valor = 0 Then
		MsgErro = "Informe o Valor."
		Exit Sub
	End If
	If Despesa.Descricao = "" Then
		MsgErro = "Informe a Descrição."
		Exit Sub
	End If
End Sub

Private Sub clsFormManter_PreencherCampos(ByRef Objeto As clsObjetoGenerico)
	Dim Despesa As clsDespesa
	Set Despesa = clsDespesa(Objeto)
	Me.FieldDescricao.Text = Despesa.Descricao
	Me.FieldValor.Text = CStr(Despesa.Valor)
	Me.FieldKM.Text = Despesa.KM
	Me.FieldObservacoes.Text = Despesa.Observacoes
	Me.SelectorData.Date = Despesa.Data
	Me.SelectorHora.Time = Despesa.Hora
	Me.Identificador = Despesa.Id
	Me.PopupLocal.ListIndex = Me.PopupLocal.FindItemData(Despesa.Local.Id)
	Me.PopupTipo.ListIndex = Me.PopupTipo.FindItemData(Despesa.Tipo.Id)
	Me.PopupVeiculos.ListIndex = Me.PopupVeiculos.FindItemData(Despesa.Veiculo.Id)
	Me.FieldProximaDespesa.Text = CStr(Despesa.ProximaDespesa)
End Sub

Private Sub btnSalvar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Salvar()
End Sub

Private Sub btnExcluir_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Excluir()
End Sub

Private Sub btnVoltar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.Voltar()
End Sub

Private Sub clsFormManter_LimparCampos()
	Me.FieldDescricao.Text = ""
	Me.FieldValor.Text = ""
	Me.FieldKM.Text = ""
	Me.FieldObservacoes.Text = ""
	Me.SelectorData.Date = Now()
	Me.PopupLocal.ListIndex = 0
	Me.PopupTipo.ListIndex = 0
	Me.Identificador = 0
	Me.Estado = 1
End Sub

Private Sub clsFormManter_TelaListagem(ByRef Tela As clsRepassador)
	Set Tela.Objeto = New FormListarDespesas
End Sub

Private Sub btnObservacoes_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormNotas
	Set Form.FieldPreencher = Me.FieldObservacoes
	Form.Show(hbFormModeless+hbFormPopup)
End Sub

Private Sub btnAdicionarPosto_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormManterEmpresa
	Dim Empresa As clsEmpresa
	Form.IsModal = True
	Form.Show(hbFormModal+hbFormPopup)
	If Not Form.Objeto Is Nothing Then
		Set Empresa = clsEmpresa(Form.Objeto)
		ObterPostos(Me.PopupLocal,Empresa.Nome)
	End If
End Sub

Private Sub btnAdicionarTipo_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormManterTipoDespesa
	Dim Tipo As clsTipoDespesa
	Form.IsModal = True
	Form.Show(hbFormModal+hbFormPopup)
	If Not Form.Objeto Is Nothing Then
		Set Tipo = clsTipoDespesa(Form.Objeto)
		ObterTiposDespesas(Me.PopupTipo,Tipo.Descricao)
	End If
End Sub

Private Sub btnNovo_PenDown(ByVal x As Integer, ByVal y As Integer)
	Me.LimparCampos()
End Sub

Private Sub btnAgendar_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormAgendaKM
	Form.FieldOrigem = Me.FieldProximaDespesa
	Form.Show(hbFormPopup)
End Sub

Private Sub btnCalculadora_PenDown(ByVal x As Integer, ByVal y As Integer)
	Dim Form As New FormCalculadora
	Form.FieldRetorno = Me.FieldValor
	Form.Show(hbFormModal+hbFormPopup)
End Sub]]></Source>
