Private Sub PopupVeiculo_Change()
	If Me.PopupVeiculo.ListIndex > -1 Then
		Dim Veiculo As Long
		Dim CM As Double
		Veiculo = Me.PopupVeiculo.ItemData(Me.PopupVeiculo.ListIndex)
		CM = RNFactory.RNPercursos().ObterConsumoMedioPorVeiculo(Veiculo)
		Me.FieldCM.Text = Format(CM,"0.0000") & " KMs/Litro"
	End If
End Sub

Private Sub Form_Load()
	ObterVeiculos(Me.PopupVeiculo)
