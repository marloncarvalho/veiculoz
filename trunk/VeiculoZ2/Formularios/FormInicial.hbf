Private Sub MenuMotorista_Click()
	Dim Motorista As clsMotorista
	Dim Form As New FormManterMotorista
	Motorista = clsMotorista(RNFactory.RNMotorista().ObterUltimo())
	If Not Motorista Is Nothing Then
		Form.Identificador = Motorista.Id
	End If
	Form.Iniciar()
	Form.Show(hbFormGoto+hbFormModal)
End Sub

