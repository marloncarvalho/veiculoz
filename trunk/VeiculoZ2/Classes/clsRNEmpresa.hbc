Private Sub Initialize()
	Set Me.Dao = New clsDAOEmpresa
End Sub

Private Sub clsRNGenerico_ValidarExclusao(ByRef Id As Long, ByRef Mensagem As String)
	If RNFactory.RNAbastecimentos().ExisteAbastecimentoPosto(Id) Then
		Mensagem = "Posto não pode ser excluído, pois está associado a um abastecimento."
		Exit Sub
	End If
	If RNFactory.RNDespesas().ExisteDespesaEmpresa(Id) Then
		Mensagem = "Local não pode ser excluído, pois está associado a uma Despesa."
		Exit Sub
	End If
End Sub]]></Source>
</Class>
