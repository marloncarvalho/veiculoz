Private Sub Form_Load()
	Dim Colecao As Collection
	Dim Tipo As clsTipoCombustivel
	Set Colecao = Factory.ObterDaoTipoCombustivel().ObterTiposCombustivel()
	For Each Tipo In Colecao
		Call gridTipos.AddItem(Tipo.Id)
	Next
End Sub

Private Sub btnNovo_Click()
	Dim frm As New frmManterTipoCombustivel
	frm.bNew = True
	frm.Show hbFormModeless+hbFormGoto
End Sub

Private Sub gridTipos_Click(ByVal iRow As Integer, ByVal iCol As Integer, ByVal lItemData As Long)
	Dim Tipo As clsTipoCombustivel
	Dim Dao As clsDaoTipoCombustivel
	Set Dao = Factory.ObterDaoTipoCombustivel()
	Set Tipo = Dao.ObterTipoCombustivel(lItemData)
	Dim frm As New frmManterTipoCombustivel
	Set Dao = Nothing
	frm.bNew = False
	frm.IdEdicao = Tipo.Id
	Set Tipo = Nothing
	frm.Show hbFormModeless+hbFormGoto
End Sub

Private Sub gridTipos_LoadItem(ByVal iRow As Integer, ByVal lItemData As Long, ByVal vValue() As Variant)
	Dim Tipo As clsTipoCombustivel
	Set Tipo =Factory.ObterDaoTipoCombustivel().ObterTipoCombustivel(lItemData)
	vValue(0) = Tipo.Nome
End Sub

Private Sub btnVoltar_Click()
	Dim frm As New frmMain
	frm.Show hbFormModeless+hbFormGoto
End Sub
